{
    "compilerOptions": {
        "target": "ESNext",
        "module": "commonjs",
        "strictNullChecks": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "sourceMap": true,
        "outDir": "dist",
        "lib": ["esnext"],
        "resolveJsonModule": true
    }
}
